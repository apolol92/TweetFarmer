<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Just a Moment..">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label depthTest="DISABLE" text="To Access Twitter you need your Twitter-Keys.">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </Label>
      <Label text="Consumer Key" />
      <PasswordField>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </PasswordField>
      <Label text="Consumer Secret" />
      <PasswordField>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </PasswordField>
      <Label text="Access Token" />
      <TextField>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </TextField>
      <Label text="Access Token Secret" />
      <PasswordField>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </PasswordField>
      <HBox alignment="TOP_RIGHT">
         <children>
            <Button mnemonicParsing="false" text="Setup" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</VBox>
